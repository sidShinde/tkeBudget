FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      postProcessingDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


tkeBudgetDict
{
  type            tkeBudget;
  libs ("libTkeBudget.so");
  enabled         true;
  
  Re              6675;

  //writeControl   timeStep;
  //writeInterval  1;

  timeStart        0.6;
  timeEnd          2.0;
  writeControl    writeTime;
  
  fields
    (
     Ck    // convection
     Pk    // production
     Tk    // turbulence transport
     Dk    // viscous diffusion
     Epik  // viscous dissipation
     Pik   // velocity-gradient transport
     );
}

